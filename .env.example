# Configuration générale
ENVIRONMENT=production

# Sécurité
JWT_SECRET=your_jwt_secret_key_here

# CORS
CORS_ORIGINS=https://your-frontend-app.com,https://another-allowed-domain.com

# Connexions à la base de données
DB_POOL_SIZE=20
DB_POOL_TIMEOUT=30

# Requêtes HTTP
HTTP_TIMEOUT=60

# Cache
ENABLE_CACHE=true
CACHE_TTL=600

# Logging
LOG_LEVEL=INFO

# Limites de fichiers
MAX_UPLOAD_SIZE=100000000

# Transcription
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here
DEFAULT_LANGUAGE=fr
SPEAKER_LABELS=true
